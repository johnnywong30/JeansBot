{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "quotes": ["error", "single"],
    "semi": ["error", "never"],
    "object-curly-spacing": "always",
    "arrow-spacing": "always",
    "array-bracket-spacing": "always"
  }
}
